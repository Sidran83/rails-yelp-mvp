<%= simple_form_for @restaurant do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.input :category, collection: Restaurant::CATEGORY %>
  <%= f.submit "submit restaurant", class: "btn btn-primary" %>
<% end %>
<br>
<p><%= link_to "< back", restaurants_path %></p>

